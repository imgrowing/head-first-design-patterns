<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss.SSS}] %-5level [%thread] [%-35.35(%logger{25}.%method:%line)] - %msg%n</pattern>
<!--            <pattern>[%d{HH:mm:ss.SSS}] %-5level [%thread] [%-30.30logger{36}:%-3.3line] - %msg%n</pattern>-->
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
